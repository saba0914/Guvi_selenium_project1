#TC-3
from selenium import webdriver
from selenium.webdriver.common.by import By
import time

driver = webdriver.Chrome()
url = "https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"
driver.get(url)
driver.maximize_window()
time.sleep(4)
username_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[1]/div/div[2]/input"
username = driver.find_element(by=By.XPATH, value=username_xpath)
username.send_keys("Admin")
password_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[2]/div/div[2]/input"
password = driver.find_element(by=By.XPATH, value=password_xpath)
password.send_keys("admin123")
login_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[3]/button"
login = driver.find_element(by=By.XPATH, value=login_xpath)
login.click()
time.sleep(10)

pim_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li[2]/a"
pim = driver.find_element(by=By.XPATH, value=pim_xpath)
pim.click()
time.sleep(5)

add_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div[2]/div[1]/button"
add = driver.find_element(by=By.XPATH, value=add_xpath)
add.click()
time.sleep(4)

fname_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[1]/div[1]/div/div/div[2]/div[1]/div[2]/input"
fname = driver.find_element(by=By.XPATH, value=fname_xpath)
fname.send_keys("sab")
time.sleep(4)

rbutton_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[2]/div/label/span"
rbutton = driver.find_element(by=By.XPATH, value=rbutton_xpath)
rbutton.click()
time.sleep(4)

uname_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[3]/div/div[1]/div/div[2]/input"
uname = driver.find_element(by=By.XPATH, value=uname_xpath)
uname.send_keys("sabarirr")
time.sleep(3)

lname_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[1]/div[1]/div/div/div[2]/div[3]/div[2]/input"
lname = driver.find_element(by=By.XPATH, value=lname_xpath)
lname.send_keys("rraj")
time.sleep(3)

pwrd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[4]/div/div[1]/div/div[2]/input"
pwrd = driver.find_element(by=By.XPATH, value=pwrd_xpath)
pwrd.send_keys("Sha@@123")
time.sleep(3)

ppwrd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[1]/div[2]/div[4]/div/div[2]/div/div[2]/input"
ppwrd = driver.find_element(by=By.XPATH, value=ppwrd_xpath)
ppwrd.send_keys("Sha@@123")
time.sleep(3)

save_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/form/div[2]/button[2]"
save = driver.find_element(by=By.XPATH, value=save_xpath)
save.click()
time.sleep(4)

#TC-4

from selenium import webdriver
from selenium.webdriver.common.by import By
import time

driver = webdriver.Chrome()
url = "https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"
driver.get(url)
driver.maximize_window()
time.sleep(4)
username_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[1]/div/div[2]/input"
username = driver.find_element(by=By.XPATH, value=username_xpath)
username.send_keys("Admin")
password_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[2]/div/div[2]/input"
password = driver.find_element(by=By.XPATH, value=password_xpath)
password.send_keys("admin123")
login_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[3]/button"
login = driver.find_element(by=By.XPATH, value=login_xpath)
login.click()
time.sleep(10)

pim_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li[2]/a"
pim = driver.find_element(by=By.XPATH, value=pim_xpath)
pim.click()
time.sleep(5)

sab_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div[2]/div[3]/div/div/div[49]/div"
sab = driver.find_element(by=By.XPATH, value=sab_xpath)
sab.click()
time.sleep(4)

pd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[1]/div[2]/div[1]/a"
pd = driver.find_element(by=By.XPATH, value=pd_xpath)
pd.click()
time.sleep(2)

cd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[1]/div[2]/div[2]/a"
