#TC-1

from selenium import webdriver
from selenium.webdriver.common.by import By
import time
driver = webdriver.Chrome()
url = "https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"
driver.get(url)
driver.maximize_window()
time.sleep(4)
username_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[1]/div/div[2]/input"
username = driver.find_element(by=By.XPATH, value=username_xpath)
username.send_keys("Admin")
password_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[2]/div/div[2]/input"
password = driver.find_element(by=By.XPATH, value=password_xpath)
password.send_keys("admin123")
login_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[3]/button"
login = driver.find_element(by=By.XPATH, value=login_xpath)
login.click()
time.sleep(10)

search_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search = driver.find_element(by=By.XPATH, value=search_xpath)
search.send_keys("admin")
admin_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a"
admin = driver.find_element(by=By.XPATH, value=admin_xpath)
admin.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("pim")
pim_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a"
pim = driver.find_element(by=By.XPATH, value=pim_xpath)
pim.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("leave")
leave_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a"
leave = driver.find_element(by=By.XPATH, value=leave_xpath)
leave.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("time")
tim_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a"
tim = driver.find_element(by=By.XPATH, value=tim_xpath)
tim.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("recruitment")
rec_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a/span"
rec = driver.find_element(by=By.XPATH, value=rec_xpath)
rec.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("my info")
myinfo_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a/span"
myinfo = driver.find_element(by=By.XPATH, value=myinfo_xpath)
myinfo.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("performance")
performance_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a/span"
performance = driver.find_element(by=By.XPATH, value=performance_xpath)
performance.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("dashboard")
das_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a/span"
das = driver.find_element(by=By.XPATH, value=das_xpath)
das.click()
time.sleep(5)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
search1 = driver.find_element(by=By.XPATH, value=search1_xpath)
search1.send_keys("maintenance")
maint_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li/a/span"
maint = driver.find_element(by=By.XPATH, value=maint_xpath)
maint.click()
time.sleep(5)
passwor_xpath = "/html/body/div/div[1]/div[1]/form/div[3]/div/div[2]/input"
passwor = driver.find_element(by=By.XPATH, value=passwor_xpath)
passwor.send_keys("admin123")

log_xpath = "/html/body/div/div[1]/div[1]/form/div[4]/button[2]"
log = driver.find_element(by=By.XPATH, value=log_xpath)
log.click()
time.sleep(10)

search1_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/div/div/input"
