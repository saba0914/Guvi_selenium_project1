sta = driver.find_element(by=By.XPATH, value=sta_xpath)
sta.send_keys("Tamilnadu")

savve_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[2]/div[1]/form/div[4]/button"
savve = driver.find_element(by=By.XPATH, value=savve_xpath)
savve.click()

