from selenium import webdriver
from selenium.webdriver.common.by import By
import time

driver = webdriver.Chrome()
url = "https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"
driver.get(url)
driver.maximize_window()
time.sleep(4)
username_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[1]/div/div[2]/input"
username = driver.find_element(by=By.XPATH, value=username_xpath)
username.send_keys("Admin")
password_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[2]/div/div[2]/input"
password = driver.find_element(by=By.XPATH, value=password_xpath)
password.send_keys("admin123")
login_xpath = "/html/body/div/div[1]/div/div[1]/div/div[2]/div[2]/form/div[3]/button"
login = driver.find_element(by=By.XPATH, value=login_xpath)
login.click()
time.sleep(10)

pim_xpath = "/html/body/div/div[1]/div[1]/aside/nav/div[2]/ul/li[2]/a"
pim = driver.find_element(by=By.XPATH, value=pim_xpath)
pim.click()
time.sleep(5)

sab_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div[2]/div[3]/div/div/div[49]/div"
sab = driver.find_element(by=By.XPATH, value=sab_xpath)
sab.click()
time.sleep(4)

pd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[1]/div[2]/div[1]/a"
pd = driver.find_element(by=By.XPATH, value=pd_xpath)
pd.click()
time.sleep(2)

cd_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[1]/div[2]/div[2]/a"
cd = driver.find_element(by=By.XPATH, value=cd_xpath)
cd.click()
time.sleep(2)

st_xpath = "/html/body/div/div[1]/div[2]/div[2]/div/div/div/div[2]/div[1]/form/div[1]/div/div[1]/div/div[2]/input"
st = driver.find_element(by=By.XPATH, value=st_xpath)
st.send_keys("raj nagar")

